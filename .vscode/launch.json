  "version": "0.2.0",
  "configurations": [
    {
      "type": "chrome",
      "request": "launch",
      "name": "debugger umi",
      "url": "http://localhost:8002",
      "webRoot": "${workspaceFolder}/src",
      "sourceMaps": true,
      "sourceMapPathOverrides": {
        "webpack:///src/*": "${webRoot}/*",

        "webpack:///./*": "${webRoot}/*",
        "webpack:///*": "*",
        "src:///src/*": "${webRoot}/*"
      },
      "resolveSourceMapLocations": [
        "${workspaceFolder}/dist/*"
        // "${workspaceFolder}/src/renderer/.umi/electron/**"
      ]
      // "autoAttachChildProcesses": true
    }
  ]
}

